{
    "name": "edneymatias/antigravity-laravel-kit",
    "description": "AI Agent templates for Laravel development - Skills, Agents, and Workflows for enhanced coding assistance",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Edney Matias"
        }
    ],
    "homepage": "https://github.com/edneymatias/antigravity-laravel-kit",
    "support": {
        "issues": "https://github.com/edneymatias/antigravity-laravel-kit/issues",
        "source": "https://github.com/edneymatias/antigravity-laravel-kit"
    },
    "keywords": [
        "antigravity",
        "ai",
        "agent",
        "gemini",
        "laravel",
        "livewire",
        "skills",
        "templates"
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Antigravity\\LaravelKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Antigravity\\LaravelKit\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Antigravity\\LaravelKit\\AntigravityServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Antigravity\\LaravelKit\\Installer::postInstall"
        ],
        "post-update-cmd": [
            "Antigravity\\LaravelKit\\Installer::postUpdate"
        ],
        "test": "pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}